<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.image.ImageView?>

<VBox stylesheets="@styles.css" styleClass="vbox" xmlns:fx="http://javafx.com/fxml" fx:controller="org.example._pngnp.HelloController">

  <padding>
    <Insets bottom="20.0" left="20.0" right="20.0" top="20.0"/>
  </padding>

  <ImageView styleClass="logo" fx:id="logoImageView" fitWidth="200" fitHeight="200" preserveRatio="true" />
  <Label styleClass="title" text="PNGNP" />
  <Button styleClass="button" text="Start" onAction="#onStartButtonClick" />

</VBox>